<!--home.ejs-->
<div class="row">
    <% houses.forEach( function(model) { %>
        <% if (model.HighLighted=="TRUE") { %>
            <div class="card col-sm-12 col-md-6">
                <div class="card-body">
                    <h5 class="card-title">
                        <a href="/HOUSE/details/<%= model.id %>">
                            <%= model.name %>
                        </a>
                    </h5>
                    <div class="row">
                        <div class="col-sm-12 col-md-5">
                            <img src="<%= model.ImageURL %>" alt="loading..." class="card-img">
                        </div>
                        <div class="card col-sm-12 col-md-7">
                            <ul class="list-group list-group-flush">
                                <li class="list-group-item"> Estate:
                                    <%= model.Estate %>
                                </li>
                                <li class="list-group-item"> Gross Area:
                                    <%= model.GrossArea %>
                                </li>
                                <li class="list-group-item"> Rent:
                                    <%= model.Rent %>
                                </li>
                            </ul>
                        </div>

                    </div>
                </div>

            </div>

            <% }; %>

                <% }); %>
</div>