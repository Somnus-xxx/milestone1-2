<!--MyRentals.ejs-->
<div class="row">
    <% myhouses.forEach( function(models) { %>
    <div class="card col-sm-12 col-md-6">
        <div class="card-body">
            <h5 class="card-title">
                <a href="/HOUSE/details/<%= models.id %>">
                    <%= models.name %>
                </a>
            </h5>
            <div class="row">
                <div class="col-sm-12 col-md-5">
                    <img src="<%= models.ImageURL %>" alt="loading..." class="card-img">
                </div>
                <div class="card col-sm-12 col-md-7">
                    <ul class="list-group list-group-flush">
                        <li class="list-group-item"> Estate:
                            <%= models.Estate %>
                        </li>
                        <li class="list-group-item"> Gross Area:
                            <%= models.GrossArea %>
                        </li>
                        <li class="list-group-item"> Rent:
                            <%= models.Rent %>
                        </li>
                    </ul>
                </div>

            </div>
        </div>
    </div>
    <% }); %>
</div>